{
	"pug": true,
	"httpSecure": "security/server",
	"variation": "common.json",
	"assetsRelativePath": "public",
	"controllersRelativePath": "routes",
	"pageNotFound": "/error",
	"routes": {
		"/": "index.pug",
		"/users/(:member/)?": {
			"controller": "users.js",
			"mimeType": "application/json"
		},
		"/error": {
			"view": "error.pug",
			"variation": "error.json",
			"statusCode": 404
		}
	}
}